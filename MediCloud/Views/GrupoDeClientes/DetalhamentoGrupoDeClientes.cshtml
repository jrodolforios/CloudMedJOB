                                                    $('#idCidadeEntrega').attr('value', 0);  // clear the textbox
                                                }
                                            });

                                            function bloquearCrediario(codigo) {
                                                var bloquear = false;
                                                if ($("#" + codigo).find("#bloqueado").children().length > 0) {
                                                    $("#" + codigo).find("#bloqueado").find("#spanBloqueado").remove();
                                                    bloquear = false;
                                                } else {
                                                    $("#" + codigo).find("#bloqueado").html("<span class=\"label label-danger\" id=\"spanBloqueado\">Bloqueado</span>");
                                                    bloquear = true;
                                                }

                                                bloquearCrediarioAjax(bloquear, codigo);
                                            }

                                            function bloquearCrediarioAjax(bloquear, codigo) {
                                                $.ajax({
                                                    url: '/CrediarioCliente/BloquearCrediario?codigoDoCrediario=' + codigo + "&Bloquear=" + bloquear,
                                                    type: "post",
                                                    cache: false,
                                                    success: function (savingStatus) {
                                                        var tipoAlert;
                                                        if (savingStatus.acaoBemSucedida)
                                                            tipoAlert = 'alert-success';
                                                        else
                                                            tipoAlert = 'alert-danger';
                                                        $('#GrupoDeCLientesAlerts').html('<div class="alert ' + tipoAlert + '"><a class="close" data-dismiss="alert">×</a><span>' + savingStatus.mensagem + '</span></div>');
                                                    },
                                                    error: function (xhr, ajaxOptions, thrownError) {
                                                        $('#GrupoDeCLientesAlerts').html('<div class="alert alert-danger"><a class="close" data-dismiss="alert">×</a><span>Ops! Não foi possível bloquear este usuário no servidor. Contate-nos para que possamos te ajudar melhor.</span></div>')
                                                    }
                                                });
                                            }

                                            $(".tooltipASO").tooltip({
                                                show: null,
                                                position: {
                                                    my: "left top",
                                                    at: "left bottom"
                                                },
                                                open: function (event, ui) {
                                                    ui.tooltip.animate({ top: ui.tooltip.position().top + 10 }, "fast");
                                                }
                                            });

                                            function bloquearGrupo(codigo) {
                                                $.ajax({
                                                    url: '/GrupoDeClientes/BloquearCrediarioDeGrupo?codigoDoGrupo=' + codigo,
                                                    type: "post",
                                                    cache: false,
                                                    success: function (savingStatus) {
                                                        var tipoAlert;
                                                        if (savingStatus.acaoBemSucedida)
                                                            tipoAlert = 'alert-success';
                                                        else
                                                            tipoAlert = 'alert-danger';

                                                        $('#Crediarios').load(document.URL + ' #Crediarios');


                                                        $('#GrupoDeCLientesAlerts').html('<div class="alert ' + tipoAlert + '"><a class="close" data-dismiss="alert">×</a><span>' + savingStatus.mensagem + '</span></div>');
                                                    },
                                                    error: function (xhr, ajaxOptions, thrownError) {
                                                        $('#GrupoDeCLientesAlerts').html('<div class="alert alert-danger"><a class="close" data-dismiss="alert">×</a><span>Ops! Não foi possível bloquear este usuário no servidor. Contate-nos para que possamos te ajudar melhor.</span></div>')
                                                    }
                                                });
                                            }
                                            
</script>
