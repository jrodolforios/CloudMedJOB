        $("#idProfissional").attr('value', '');
        $("#profissional").attr('value', '');
        $("#dataExame").attr('value', new Date().toLocaleDateString());
        $("#usuarioProcedimento").attr('value', '');
        $("#observacoesProcedimento").val('');
        $("#valor").val('');
        $("#desconto").val('');
        $("#dataProxExame").val('');
        $("#total").val('');
    }

    function recalcularTotal() {
        var valorProcedimento = parseFloat($('#valor').val());
        var valorDesconto = parseFloat($('#desconto').val());

        $('#total').val(parseFloat(Math.round(((valorProcedimento ? valorProcedimento : 0) - (valorDesconto ? valorDesconto : 0)) * 100) / 100).toFixed(2));

        //Garantir 2 casas decimais sempre
        $('#valor').val(parseFloat(Math.round((valorProcedimento ? valorProcedimento : 0) * 100) / 100).toFixed(2));
        $('#desconto').val(parseFloat(Math.round((valorDesconto ? valorDesconto : 0) * 100) / 100).toFixed(2));
    }

    $('#desconto').on('blur', function () {
        recalcularTotal();
